{
    "name": "Digital Clock",
    "description": "Simple Digital Clock Plugin For Chrome!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "/assets/clock16.png",
        "32": "/assets/clock32.png",
        "48": "/assets/clock48.png",
        "128": "/assets/clock128.png"
      }
    },
    "icons": {
      "16": "/assets/clock16.png",
      "32": "/assets/clock32.png",
      "48": "/assets/clock48.png",
      "128": "/assets/clock128.png"
    }
  }